#include <iostream>
#include <vector>
using namespace std;
void sort(float array[], int size)
{
   for(float i = 0; i < size - 1; i++)
   {
      for(float j = 0; j < size - i - 1; j++)
      {
         if(array[j] > array[j+1])
         {
            float temp = array[j];
            array[j] = array[j+1];
            array[j+1] = temp;
         }
      }
   }
}

void printArray(float array[], int size)
{
   for(float i = 0; i < size; i++)
   {
        cout<<array[i];
   }
}

int main(){
    vector<float> v = {7, 5, 16, 8};
}
